   },
   ".tab .tab-actions": {
      "opacity": "0 !important",
      "transition": "opacity 0.2s cubic-bezier(0.4, 0, 0.2, 1) !important"
   },
   ".tab:hover .tab-actions": {
      "opacity": "1 !important"
   },
   ".tab.active .tab-actions": {
      "opacity": "1 !important"
